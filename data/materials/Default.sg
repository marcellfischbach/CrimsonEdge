shaderGraph {

    receiveShadow 1,
    queue Default,
    blend Off,

    nodes {
        resource "cryo::SGResourceFloat" roughness "Roughness",

        resource "cryo::SGResourceColor3" diffuse "Diffuse",

        resource "cryo::SGTexture2D" normal2 "Normal",

        node "cryo::SGConstFloat" alpha {
            value 0 1.0,
        },

        node "cryo::SGConstColor3" normal {
            value 0 0.5,
            value 1 0.5,
            value 2 1.0,
        },

        node "cryo::SGDecomposeVec4" normalDec {
            binding "V" normal2,
        }

        node "cryo::SGVec3" normalRec {
            binding "X" normalDec 0,
            binding "Y" normalDec 1,
            binding "Z" normalDec 2,
        }

        shaderGraph {
            binding "Diffuse" diffuse,
            binding "Alpha" alpha,
            binding "Roughness" roughness,
            binding "Normal" normalRec,
        }
    },

    attributes {
        attribute Float "Roughness" 0.0,
        attribute Texture "Normal" "/textures/fieldstone_normal.tex2d",
    }


},